{
  "name": "alv-portal-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:staging",
    "start:dev": "npm run i18n && ng serve --base-href / --watch --proxy-config proxy.conf.dev.json",
    "start:staging": "npm run i18n && ng serve --base-href / --watch --proxy-config proxy.conf.staging.json",
    "start:mobile": "npm run i18n && ng serve --host 0.0.0.0 --base-href / --watch --proxy-config proxy.conf.staging.json",
    "start:prod": "npm run i18n && ng serve --base-href / --watch --prod --aot --proxy-config proxy.conf.staging.json",
    "build": "npm run build:dev",
    "build:dev": "npm run i18n && ng build --base-href /",
    "build:prod": "npm run i18n && ng build --base-href / --aot --prod --stats-json",
    "test": "ng test --source-map=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "npm cache verify && rimraf -- node_modules doc target/classes",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "docs:compodoc": "compodoc -p tsconfig.json",
    "docs:compodoc:serve": "compodoc -p tsconfig.json -s",
    "docs:compodoc:serve:watch": "compodoc -p tsconfig.json -s -w",
    "postinstall": "node version.js",
    "extract": "ngx-translate-extract --input ./src/app --output ./src/assets/i18n/{en,fr,de,it}.json --clean --sort --format namespaced-json --format-indentation ' ' ",
    "i18n": "node pipeline-scripts/csv2json-i18n.js translations.csv --output src/assets/i18n",
    "i18n:duplicate-problems": "node ./pipeline-scripts/csv_check_duplicates.js ../translations.csv",
    "i18n:column-numbers": "node pipeline-scripts/csv_check_column_numbers.js ../translations.csv",
    "i18n:missing-translations": "node pipeline-scripts/csv_check_missing.js translations.csv",
    "i18n:clashes": "node pipeline-scripts/csv_check_clashes.js ../translations.csv",
    "bundle-report": "webpack-bundle-analyzer target/classes/ch/admin/seco/alvportal/ui/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.10",
    "@angular/common": "7.2.10",
    "@angular/compiler": "7.2.10",
    "@angular/core": "7.2.10",
    "@angular/forms": "7.2.10",
    "@angular/http": "7.2.10",
    "@angular/platform-browser": "7.2.10",
    "@angular/platform-browser-dynamic": "7.2.10",
    "@angular/router": "7.2.10",
    "@fortawesome/free-solid-svg-icons": "5.8.0",
    "@fortawesome/fontawesome-svg-core": "1.2.16",
    "@fortawesome/angular-fontawesome": "0.3.0",
    "@ng-bootstrap/ng-bootstrap": "4.1.0",
    "@ngrx/core": "1.2.0",
    "@ngrx/effects": "7.3.0",
    "@ngrx/router-store": "7.3.0",
    "@ngrx/store": "7.3.0",
    "@ngrx/store-devtools": "7.3.0",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@types/dom-inputevent": "1.0.4",
    "@types/jwt-decode": "2.2.1",
    "@types/lodash": "4.14.123",
    "@types/lodash-es": "4.17.3",
    "@types/pretty-bytes": "5.1.0",
    "angular2-prettyjson": "3.0.1",
    "bootstrap": "4.3.1",
    "core-js": "3.0.0",
    "i18n-iso-countries": "3.7.8",
    "jwt-decode": "2.2.0",
    "libphonenumber-js": "1.7.12",
    "ng2-file-upload": "1.3.0",
    "ng2-nouislider": "1.7.13",
    "ngx-clipboard": "12.0.0",
    "ngx-cookie-service": "2.1.0",
    "ngx-infinite-scroll": "7.1.0",
    "ngx-markdown": "7.1.4",
    "nodelist-foreach-polyfill": "1.2.0",
    "nouislider": "13.1.4",
    "open-sans-fonts": "1.5.1",
    "pretty-bytes": "5.1.0",
    "rxjs": "6.4.0",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.6",
    "@angular/cli": "7.3.6",
    "@angular/compiler-cli": "7.2.10",
    "@angular/language-service": "7.2.10",
    "@biesbjerg/ngx-translate-extract": "2.3.4",
    "@compodoc/compodoc": "1.1.9",
    "@types/jasmine": "3.3.12",
    "@types/jasmine-matchers": "0.2.32",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "11.11.4",
    "codelyzer": "5.0.0-beta.1",
    "fs-extra": "7.0.1",
    "git-describe": "4.0.4",
    "jasmine-core": "3.3.0",
    "jasmine-expect": "4.0.1",
    "jasmine-marbles": "0.4.1",
    "jasmine-spec-reporter": "4.2.1",
    "json-stable-stringify": "https://github.com/ganqqwerty/json-stable-stringify",
    "karma": "4.0.1",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-firefox-launcher": "1.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.0",
    "lodash": "4.17.11",
    "minimist": "1.2.0",
    "papaparse": "4.6.3",
    "protractor": "5.4.2",
    "rimraf": "2.6.3",
    "rxjs-tslint-rules": "4.19.0",
    "shelljs": "0.8.3",
    "source-map-explorer": "1.8.0",
    "ts-node": "8.0.3",
    "tslint": "5.14.0",
    "tslint-angular": "1.1.2",
    "typescript": "3.2.4",
    "webpack-bundle-analyzer": "3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alv-ch/alv-portal.git"
  },
  "//": [
    "rimraf, crossenv and crossvar are needed for cross-browser compatibility of the npm scripts (including Windows)"
  ],
  "engines": {
    "node": ">= 10.6.0",
    "npm": ">= 6"
  }
}
