{
  "name": "alv-portal-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "ng-update": "ng update @angular/cli @angular/core --allow-dirty",
    "start": "npm run start:staging",
    "start:dev": "npm run i18n && ng serve --base-href / --watch --proxy-config proxy.conf.dev.json",
    "start:staging": "npm run i18n && ng serve --base-href / --watch --proxy-config proxy.conf.staging.json --host 0.0.0.0",
    "start:prod": "npm run i18n && ng serve --base-href / --watch --prod --aot --proxy-config proxy.conf.staging.json",
    "build": "npm run build:dev",
    "build:dev": "npm run i18n && ng build --base-href /",
    "build:prod": "npm run i18n && ng build --base-href / --aot --prod --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e --host=127.0.0.1",
    "clean": "npm cache verify && rimraf -- node_modules doc target/classes package-lock.json",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "docs:compodoc": "compodoc -p tsconfig.json",
    "docs:compodoc:serve": "compodoc -p tsconfig.json -s",
    "docs:compodoc:serve:watch": "compodoc -p tsconfig.json -s -w",
    "postinstall": "node version.js",
    "extract": "ngx-translate-extract --input ./src/app --output ./src/assets/i18n/{en,fr,de,it}.json --clean --sort --format namespaced-json --format-indentation ' ' ",
    "i18n": "node pipeline-scripts/csv2json-i18n.js translations.csv --output src/assets/i18n",
    "i18n:duplicate-problems": "node ./pipeline-scripts/csv_check_duplicates.js ../translations.csv",
    "i18n:column-numbers": "node pipeline-scripts/csv_check_column_numbers.js ../translations.csv",
    "i18n:missing-translations": "node pipeline-scripts/csv_check_missing.js translations.csv",
    "i18n:clashes": "node pipeline-scripts/csv_check_clashes.js ../translations.csv",
    "bundle-report": "webpack-bundle-analyzer target/classes/ch/admin/seco/alvportal/ui/stats.json",
    "webdriver-update": "webdriver-manager update"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "8.1.0",
    "@angular/common": "8.1.0",
    "@angular/compiler": "8.1.0",
    "@angular/core": "8.1.0",
    "@angular/forms": "8.1.0",
    "@angular/http": "7.2.15",
    "@angular/platform-browser": "8.1.0",
    "@angular/platform-browser-dynamic": "8.1.0",
    "@angular/router": "8.1.0",
    "@fortawesome/angular-fontawesome": "0.4.0",
    "@fortawesome/fontawesome-pro": "5.10.1",
    "@fortawesome/fontawesome-svg-core": "1.2.19",
    "@fortawesome/free-regular-svg-icons": "5.9.0",
    "@fortawesome/free-solid-svg-icons": "5.9.0",
    "@ng-bootstrap/ng-bootstrap": "5.0.0",
    "@ngrx/effects": "8.0.1",
    "@ngrx/router-store": "8.0.1",
    "@ngrx/store": "8.0.1",
    "@ngrx/store-devtools": "8.0.1",
    "@ngx-translate/core": "11.0.1",
    "@ngx-translate/http-loader": "4.0.0",
    "@types/date-fns": "2.6.0",
    "@types/dom-inputevent": "1.0.4",
    "@types/jwt-decode": "2.2.1",
    "@types/lodash": "4.14.134",
    "@types/lodash-es": "4.17.3",
    "@types/pretty-bytes": "5.1.0",
    "@types/uuid": "3.4.4",
    "angular2-prettyjson": "3.0.1",
    "bootstrap": "4.3.1",
    "core-js": "3.1.3",
    "date-fns": "1.30.1",
    "i18n-iso-countries": "4.1.0",
    "jwt-decode": "2.2.0",
    "libphonenumber-js": "1.7.19",
    "ng-block-ui": "2.1.3",
    "ng2-file-upload": "1.3.0",
    "ng2-nouislider": "1.8.2",
    "ngx-clipboard": "12.1.0",
    "ngx-cookie-service": "2.2.0",
    "ngx-infinite-scroll": "7.2.0",
    "ngx-markdown": "8.0.2",
    "nodelist-foreach-polyfill": "1.2.0",
    "nouislider": "13.1.5",
    "open-sans-fonts": "1.5.1",
    "pretty-bytes": "5.2.0",
    "rxjs": "6.4.0",
    "uuid": "3.3.2",
    "xxhashjs": "0.2.2",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.800.3",
    "@angular/cli": "8.1.0",
    "@angular/compiler-cli": "8.1.0",
    "@angular/language-service": "8.1.0",
    "@biesbjerg/ngx-translate-extract": "2.3.4",
    "@compodoc/compodoc": "1.1.10",
    "@types/jasmine": "3.3.13",
    "@types/jasmine-matchers": "0.2.32",
    "@types/jasminewd2": "2.0.6",
    "@types/node": "12.0.7",
    "codelyzer": "5.1.0",
    "cross-env": "5.2.0",
    "cross-var": "1.1.0",
    "fs-extra": "8.0.1",
    "git-describe": "4.0.4",
    "jasmine-core": "3.4.0",
    "jasmine-expect": "4.0.1",
    "jasmine-marbles": "0.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "json-stable-stringify": "https://github.com/ganqqwerty/json-stable-stringify",
    "karma": "4.1.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.5",
    "karma-firefox-launcher": "1.1.0",
    "karma-jasmine": "2.0.1",
    "karma-jasmine-html-reporter": "1.4.2",
    "lodash": "4.17.14",
    "minimist": "1.2.0",
    "papaparse": "5.0.0",
    "protractor": "5.4.2",
    "rimraf": "2.6.3",
    "rxjs-tslint-rules": "4.24.0",
    "shelljs": "0.8.3",
    "source-map-explorer": "2.0.0",
    "ts-node": "8.2.0",
    "tslint": "5.17.0",
    "tslint-angular": "3.0.2",
    "typescript": "3.4.5",
    "webpack-bundle-analyzer": "3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alv-ch/alv-portal.git"
  },
